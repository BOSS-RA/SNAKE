import pandas as pd
import matplotlib.pyplot as plt
# Sample DataFrame
df = pd.DataFrame({
 'A': [1, 2, 3, 4, 5],
 'B': [2, 3, 4, 5, 6]
})
# Scatter Plot
plt.scatter(df['A'], df['B'])
plt.xlabel('A')
plt.ylabel('B')
plt.title('Scatter Plot')
plt.show()
# Plot
plt.plot(df['A'], df['B'])
plt.xlabel('A')
plt.ylabel('B')
plt.title('Line Plot')
plt.show()
